#!/bin/sh

copyq_clipboard() {
    for i in -1 0 1 2 3 4 5 6 7 8
    do
        index=$(( i + 1 ));
        content=`copyq tab clipboard read $index`;
        if [ "$content" != "" ]
        then
            echo "[$index] -------------------------------------------\n"
            echo $content;
            echo "\n";
        fi
    done
}

copyq_notes() {
    for i in 0 1 2 3 4 5 6 7 8
    do
        index=$(( i + 1 ));
        content=`copyq tab notes read $i`;
        if [ "$content" != "" ]
        then
            echo "[$index] -------------------------------------------\n"
            echo $content;
            echo "\n";
        fi
    done
}
